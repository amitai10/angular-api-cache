{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
        "name": "api.openweathermap.org",
        "urls": ["https://api.openweathermap.org/data/2.5/*", "https://api.openweathermap.org/data/2.5/*"],
        "cacheConfig": {
            "maxSize": 100,
            "maxAge": "1d",
            "strategy": "freshness"
        }
    }
]
}
